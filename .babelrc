{
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-runtime",
        "transform-class-properties",
        "transform-object-rest-spread",
        [
            "react-intl",
            {
                "messagesDir": "./dist/messages",
                "enforceDescriptions": false
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}